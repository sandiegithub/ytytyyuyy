        <div class="main-content" ng-controller="indexController">
            <div class="main-content-inner">
                <div class="breadcrumbs ace-save-state" id="breadcrumbs">
                    <ul class="breadcrumb">
                        <li> <i class="ace-icon fa fa-home home-icon"></i> <a href="#">@LCEAdminUI.AppGlobalResource.Resource.Home</a> </li>
                        <li class="active">@LCEAdminUI.AppGlobalResource.Resource.OrderHistorySearch</li>
                    </ul>
                </div>
                <div class="page-content">
