                                    <tr>
                                        <td><input type="checkbox" ng-model="searchDetails.SelectDeselectAll" /></td>
                                        <td width="20%"><span class="green">@LCEAdminUI.AppGlobalResource.Resource.Name:</span> {{searchDetails.Name}}</td>
                                        <td width="20%"><span class="green">@LCEAdminUI.AppGlobalResource.Resource.Order #:</span> {{searchDetails.AppOrderNumber}}</td>
                                        <td width="20%"><span class="green">@LCEAdminUI.AppGlobalResource.Resource.Store #:</span> {{searchDetails.StoreId}}</td>
                                        <td width="20%"><span class="green">@LCEAdminUI.AppGlobalResource.Resource.OrderTime:</span> {{searchDetails.DateTimeOfOrder}}</td>
                                        <td width="20%"><span class="green">@LCEAdminUI.AppGlobalResource.Resource.PickUpTime:</span> {{searchDetails.OrderPickUpTime}}</td>
                                    </tr>

                                    <tr>
                                        <td colspan="6">
                                            <div class="row m-b">
                                                <label class="col-sm-2 green text-right">@LCEAdminUI.AppGlobalResource.Resource.ListOutOrderItems :</label>
