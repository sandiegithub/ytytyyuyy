                                                <label class="col-sm-2 green text-right">@LCEAdminUI.AppGlobalResource.Resource.PickUpLocation :</label>
                                                <p class="col-sm-2">{{searchDetails.PickUpLocation}}</p>
                                            </div>
                                        </td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                        <div class="row">
