                        <div class="col-xs-12">
                            <div class="widget-box">
                                <div class="widget-header">
                                    <h4 class="widget-title">  @LCEAdminUI.AppGlobalResource.Resource.OrderHistorySearch </h4>
                                    <button type="button" class="btn btn-success pull-right"> @LCEAdminUI.AppGlobalResource.Resource.Email</button>
                                    <button type="button" class="btn btn-danger pull-right select" ng-click="SelectDeselectAll()"> @LCEAdminUI.AppGlobalResource.Resource.SelectDeselectAll </button>
                                    <button type="button" class="btn btn-danger pull-right select" ng-click="OrderSearch()"> @LCEAdminUI.AppGlobalResource.Resource.Search</button>
                                </div>
                                <div class="widget-body">
                                    <form>
                                        <div class="row">
                                            <div class="widget-main">
                                                <div class="col-lg-12">
                                                    <h5 class="green">@LCEAdminUI.AppGlobalResource.Resource.TimeStoreSearch</h5>
                                                    <div class="row">
                                                        <div class="col-lg-9 text-right">
                                                            <h5 class="green Search">@LCEAdminUI.AppGlobalResource.Resource.From</h5>
                                                        </div>
                                                        <div class="col-lg-3 text-left">
                                                            <h5 class="green Form">@LCEAdminUI.AppGlobalResource.Resource.To</h5>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-sm-3">
                                                            <select class="form-control" ng-model="orderSearchRequest.SelectedStore">
                                                                <option value="Show All Store Orders">Show All Store Orders</option>
                                                                <option ng-repeat="stores in storeInformationList" value="{{stores.StoreInformation}}">{{stores.StoreInformation}}</option>
                                                            </select>
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <select class="form-control" ng-model="orderSearchRequest.SelectDate" ng-change="onDateChange()">
                                                                <option value="Today">Today</option>
                                                                <option value="Current Week">Current Week</option>
                                                                <option value="Last 48 Hours">Last 48 Hours</option>
                                                                <option value="Past Week">Past Week</option>
                                                                <option value="Current Month">Current Month </option>
                                                                <option value="Past Month">Past Month</option>
                                                                <option value="Custom Time Frame">Custom Time Frame</option>
                                                            </select>
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <div class="input-group">
                                                                <input class="form-control date-picker" id="id-date-picker-1" type="text" ng-model="orderSearchRequest.StartDate" ng-disabled="shouldBeDisabled" class="col-xs-10 col-sm-5">
                                                                <span class="input-group-addon"> <i class="fa fa-calendar bigger-110"></i> </span>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <div class="input-group">
                                                                <input class="form-control date-picker" id="id-date-picker-2" ng-model="orderSearchRequest.EndDate" type="text" ng-disabled="shouldBeDisabled" class="col-xs-10 col-sm-5">
                                                                <span class="input-group-addon"> <i class="fa fa-calendar bigger-110"></i> </span>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="clearfix m-b-lg"></div>
                                                    <h5 class="green">@LCEAdminUI.AppGlobalResource.Resource.UserDetailSearch</h5>
                                                    <div class="row">
                                                        <div class="col-sm-3">
                                                            <select class="form-control" ng-change="onCategoryChange()" ng-model="orderSearchRequest.UserDetailSearchCategory">
                                                                <option value="First Name">First Name</option>
                                                                <option value="Last Name">Last Name</option>
                                                                <option value="EmailId">EmailId</option>
                                                                <option value="Phone Number">Phone Number</option>
                                                                <option value="Order Number">Order # </option>
                                                                <option value="Store Number">Store # (LCE Internal only)</option>
                                                            </select>
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <input type="text" id="form-field-1" ng-model="orderSearchRequest.UserDetailSearchData" placeholder="@LCEAdminUI.AppGlobalResource.Resource.UserDetailSearch" class="col-xs-12 col-sm-12">
                                                        </div>
                                                    </div>
                                                    <div class="clearfix m-t-lg"></div>
                                                    <div class="form-actions-mar form-actions text-left">
                                                        <label class="found" ng-cloak>{{numberOfOrdersFound}}</label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>
                <div class="col-lg-12">
                    <div class="widget-box">
                        <div class="panelbodyscroll">
                            <div class="table-responsive" ng-repeat="searchDetails in searchDetailsList |limitTo:15">
                                @*<div ng-if="$even" class="table-header">
                                    <div class="checkbox">
                                        <label>
                                            <input name="form-field-checkbox" type="checkbox" class="ace">
                                            <span class="lbl"> </span>
